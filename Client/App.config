<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>

  <appSettings>
    <add key="CsvPath" value="C:\Users\sniko\source\repos\virt_proc_projekat\solar_data.csv" />
    <add key="RowLimitN" value="100" />
  </appSettings>

  <system.serviceModel>
    <client>
      <endpoint
          name="SolarServiceEndpoint"
          address="net.tcp://localhost:4000/SolarService"
          binding="netTcpBinding"
          bindingConfiguration="StreamingBinding"
          contract="Common.ISolarService" />
    </client>

    <bindings>
      <netTcpBinding>
        <binding name="StreamingBinding"
                 maxReceivedMessageSize="16777216"
                 receiveTimeout="00:10:00"
                 sendTimeout="00:10:00">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>